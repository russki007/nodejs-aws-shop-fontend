{
  "name": "react-shop-cloudfront",
  "version": "0.1.0",
  "bin": {
    "react-shop-cloudfront": "/out/fullstack.js"
  },
  "scripts": {
    "build": "tsc -p tsconfig.json && tsc --build scripts",
    "watch": "tsc -w",
    "test": "jest",
    "deploy:infa": "npx cdk deploy --outputs-file ./scripts/outputs.json && npx ts-node ./scripts/setupDotEnv.ts",
    "deploy:s3": "cd scripts && npx ts-node ./deployS3.ts --directory ../../dist --invalidate true"
  },
  "devDependencies": {
    "@types/fs-extra": "^11.0.4",
    "@types/jest": "^29.5.12",
    "@types/node": "20.12.7",
    "aws-cdk": "2.144.0",
    "fs-extra": "^11.1.1",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "typescript": "~5.4.5"
  },
  "dependencies": {
    "@aws-sdk/client-cloudfront": "^3.592.0",
    "@aws-sdk/client-s3": "^3.592.0",
    "@aws-sdk/lib-storage": "^3.592.0",
    "aws-cdk-lib": "2.144.0",
    "constructs": "^10.0.0",
    "dotenv": "^16.4.5",
    "source-map-support": "^0.5.21"
  }
}
